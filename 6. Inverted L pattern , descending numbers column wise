// inverted L, column wise descending number
6 5 4 3 2 1 
6 5 4 3 2 
6 5 4 3 
6 5 4 
6 5 
6 
-------------------------------------------------------
#include <stdio.h>
#include <stdlib.h>
int main(void){
    int i,j,r;
    setbuf(stdout, NULL);
    printf("Enter r number for inverted L number pattern: ");
    scanf("%d",&r);
    for(i=1;i<=r;i++){
        for(j=r;j>=i;j--){
            printf("%d ", j);
        }
        printf("\n");
    }
    return 0;
}
