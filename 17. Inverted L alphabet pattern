// Inverted L alphabet pattern 
E E E E E 
D D D D 
C C C 
B B 
A 
--------------------------------------------------------------------
#include <stdio.h>
#include <stdlib.h>
int main(void){
	int i,j,n;
	setbuf(stdout, NULL);
	printf("Enter a number for Inverted L alphabet pattern: ");
	scanf("%d", &n);
	for(i=n; i>=1; i--){
		for(j=1; j<=i; j++){
			printf("%c ", 64+i);
		}
		printf("\n");
	}
	return 0;
}
